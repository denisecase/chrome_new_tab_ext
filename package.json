{
  "name": "chrome_new_tab_ext",
  "version": "1.0.0",
  "description": "Customized Chrome 'New Tab' Extension, which can be updated to reflect current links each semester",
  "main": "tab.html",
  "directories": {
    "test": "tests"
  },
  "dependencies": {
    "axios": "latest",
    "globals": "latest"
  },
  "devDependencies": {
    "@babel/core": "latest",
    "@babel/preset-env": "latest",
    "cypress": "latest",
    "eslint": "latest",
    "eslint-config-standard": "latest",
    "eslint-plugin-chai-friendly": "latest",
    "eslint-plugin-cypress": "latest",
    "eslint-plugin-import": "latest",
    "eslint-plugin-node": "latest",
    "eslint-plugin-promise": "latest",
    "eslint-plugin-standard": "latest",
    "husky": "latest",
    "prettier": "latest",
    "prettier-eslint": "latest",
    "pretty-quick": "latest"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  },
  "scripts": {
    "note": "You can run the first script by opening a PowerShell window here as Admin and typing: npm run prettier",
    "prettier": "prettier --write \"**/*.{js,css,html,json}\"",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "cyop": "cypress open",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run",
    "t1": "npx cypress run  --spec \"cypress/integration/dailyImage.spec.js\"",
    "t2": "npx cypress run  --spec \"cypress/integration/dateTime.spec.js\"",
    "t3": "npx cypress run  --spec \"cypress/integration/jokes.spec.js\"",
    "t4": "npx cypress run  --spec \"cypress/integration/location.spec.js\"",
    "test": "cypress open"
  },
  "repository": {
    "type": "git",
    "url": "git+https://professorcase@bitbucket.org/professorcase/chrome_new_tab_ext.git"
  },
  "keywords": [
    "Chrome",
    "new",
    "tab",
    "browser",
    "extension",
    "JavaScript",
    "HTML",
    "CSS",
    "Mocha",
    "unit",
    "testing",
    "location",
    "API"
  ],
  "author": "Denise Case",
  "license": "MIT",
  "homepage": "https://bitbucket.org/professorcase/chrome_new_tab_ext#readme"
}
