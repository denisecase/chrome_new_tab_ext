{
  "name": "NWCase: New Tab Extension",
  "short_name": "NWCaseNT",
  "version": "0.4",
  "manifest_version": 3,
  "description": "A simple Chrome new tab extension to start the semester.",
  "browser_action": {
    "default_icon": "./icon_36.png",
    "default_title": "NWCase: New Tab Extension"
  },
  "author": "Denise Case",
  "chrome_url_overrides": {
    "newtab": "tab.html"
  },
  "file_system_provider_capabilities": {
    "configurable": true,
    "multiple_mounts": true,
    "source": "network"
  },
  "homepage_url": "http://denisecase.github.io/",
  "incognito": "spanning",
  "offline_enabled": true,
  "omnibox": {
    "keyword": "aString"
  },
  "permissions": [
    "tabs",
    "geolocation"
     ],
  "host_permissions": [
    "<all_urls>",
    "https://www.bing.com/HPImageArchive.aspx?format=js&idx=0&n=1",
    "https://cdn.jsdelivr.net/npm/bootstrap@5.0.0-beta3/dist/js/bootstrap.bundle.min.js",
    "https://api.icndb.com/jokes/random?limitTo=[nerdy]",
    "https://www.bing.com/HPImageArchive.aspx?format=js&idx=0&n=1&mkt=en-US"
  ],
  "web_accessible_resources": ["favicon.ico"]
}
